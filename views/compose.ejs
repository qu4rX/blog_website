<%- include("partials/header") -%>

<h1>Compose</h1>
    <form action="/compose" method="post" class="form">
        <label for="inp1" class="form-para">Title</label>
        <input type="text" name="postTitle" id="inp1" class="form-control" autocomplete="off">
        <label for="inp2" class="input-para">Post</label>
        <textarea name="postBody" id="inp2" cols="30" rows="7" class="form-control"></textarea>
        <button type="submit" class="btn btn-primary">Publish</button>
    </form>

<%- include("partials/footer") -%>